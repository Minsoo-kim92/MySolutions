package intersection2;

import java.util.Scanner;

public class Solution {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		try(Scanner s = new Scanner(System.in))
	    {
	      int n = s.nextInt();
	      int min = 100000000;
	      int max = 0;
	      for(int i = 0; i < n; i++)
	      {
	        int a = s.nextInt();
	        int b = s.nextInt();
	        
	        
	        min = Math.min(min, a);
	        max = Math.max(max, b);
			//여기부터 작성해 주세요
	        
	        

		  }
	      
	      System.out.println(min+" "+max);
	      
		}
	}

}
