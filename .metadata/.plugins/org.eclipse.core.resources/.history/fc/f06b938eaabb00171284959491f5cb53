package intersection;

import java.util.Arrays;
import java.util.Scanner;

public class Solution {

	public static void main(String[] args) {
		try(Scanner s = new Scanner(System.in))
		{
			int x1 = s.nextInt();
			int y1 = s.nextInt();
			int x2 = s.nextInt();
			int y2 = s.nextInt();
			int x3 = s.nextInt();
			int y3 = s.nextInt();
			int x4 = s.nextInt();
			int y4 = s.nextInt();
			int x5 = s.nextInt();
			int y5 = s.nextInt();
			int x6 = s.nextInt();
			int y6 = s.nextInt();
			int sum=0;
			//여기부터 작성해 주세요
			int[][] ary = new int[10000][10000];
			for(int i=0; i<10000; i++) {
				Arrays.fill(ary[i], 0);
			}
			
			//1번 업소 는 1로 채움
			for(int i=x1; i<=x2;i++) {
				for(int j=y1; j<=y2;i++) {
					ary[i][j] = 1;
				}
			}
				
			//2번업소 0 으로 채움
			for(int i=x3; i<=x4;i++) {
				for(int j=y3; j<=y4;i++) {
					ary[i][j] = 0;
				}
			}
				
			//3번업소 0 으로 채움
			for(int i=x5; i<=x6;i++) {
				for(int j=y5; j<=y6;i++) {
					ary[i][j] = 0;
				}
			}
			
			for(int i=0; i<10000;i++) {
				for(int j=0; j<10000;j++) {
					if(ary[i][j] == 1) {
						sum++;
					}
				}
			}
			
			System.out.println(sum);
		}
	}

}
